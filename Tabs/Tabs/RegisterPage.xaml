<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tabs.RegisterPage" Title="Register">
    <ContentPage.Content>
        <StackLayout>
            <Image x:Name="myimage" WidthRequest="100" HeightRequest="100"/>
            <Entry Placeholder="User Name" HeightRequest="90" x:Name="UNEntry"/>
            <Entry Placeholder="Email" HeightRequest="90" x:Name="EmailEntry"/>
            <Entry Placeholder="Password" HeightRequest="90" x:Name="PassEntry"/>
            <Entry Placeholder="Phone" HeightRequest="90" x:Name="PhoneEntry"/>
            <Button Text="Register" Clicked="myregister"/>
            <Button Text="Already Register" Clicked="alreadyregister"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>